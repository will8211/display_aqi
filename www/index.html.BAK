<!DOCTYPE html>
<html>
    <head>
        <title>Display AQI</title>
        <!--<meta http-equiv="refresh" content="5" >-->
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    </head>
    <body>
    Hello
    <div id="status">Unset</div>
    <div id="content"></div>
    </body>

<script>
var url = 'http://aqicn.org/city/taiwan/nantou/';
var html;
$.ajax({
    type: 'GET',
    dataType: 'html',
    url: url,
    timeout: 60 * 1000

}).done(function (data) {
    var html = data;
    $("#status").html('<p>OK</p>');
    $("#content").html(html);
}).fail(function (a, b, c) {
    $("#status").html('<p>FAIL</p>');
    console.log(b + '|' + c);
});
//settings will require: <access origin="http://aqicn.org" />
</script>
</html>
